{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "gulpfile.js",
    "rollup.config.js",
    "assets/js/*.js"
  ]
}
