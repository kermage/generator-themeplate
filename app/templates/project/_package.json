{
  "name": "<%= opts.projectSlug %>",
  "title": "<%= opts.projectName %>",
  "version": "0.1.0",
  "description": "<%= opts.description %>",
  "keywords": [
    "generator-themeplate",
    "wordpress",
    "theme"
  ],
  "homepage": "<%= opts.projectURI %>",
