{
  "name": "<%= opts.projectSlug %>",
  "title": "<%= opts.projectName %>",
  "version": "0.1.0",
  "description": "<%= opts.description %>",
  "keywords": [
    "generator-themeplate",
    "wordpress",
    "theme"
  ],
  "homepage": "<%= opts.projectURI %>",
  "license": "<%= opts.license %>",
  "private": true,
  "author": {
    "name": "<%= opts.authorName %>",
    "email": "<%= opts.authorEmail %>",
    "url": "<%= opts.authorURI %>"
  },
  "main": "style.css",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@wordpress/browserslist-config": "^2.7.0",
    "@wordpress/eslint-plugin": "^7.4.0",
    "autoprefixer": "^10.2.1",
    "browser-sync": "^2.26.13",
    "cssnano": "^4.1.10",
    "eslint": "^7.18.0",
    "genetiks": "^1.5.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.7",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.6",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rollup-each": "^3.0.2",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "gulp-wp-pot": "^2.5.0",
    "minimist": "^1.2.5",
    "postcss": "^8.2.4",
    "rollup": "^2.36.2",
    "stylelint": "^13.8.0",
    "stylelint-config-wordpress": "^17.0.0",
    "typescript": "^4.1.3",
    <% if ( 'bootstrap' === opts.framework ) { %>
    "bootstrap": "^4.5.0",
    "popper.js": "^1.16.1"
    <% } else if ( 'tailwind' === opts.framework ) { %>
    "tailwindcss": "^2.0.2"
    <% } else { %>
    "normalize.css": "^8.0.1"
    <% } %>
  }
}
