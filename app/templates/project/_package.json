{
  "name": "<%= opts.projectSlug %>",
  "title": "<%= opts.themeName %>",
  "version": "0.1.0",
  "description": "<%= opts.description %>",
  "keywords": [
    "wordpress"
  ],
  "homepage": "<%= opts.themeURI %>",
  "license": "<%= opts.license %>",
  "private": true,
  "author": {
    "name": "<%= opts.authorName %>",
    "email": "<%= opts.authorEmail %>",
    "url": "<%= opts.authorURI %>"
  },
  "main": "style.css",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "browser-sync": "^2.26.3",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-bump": "^3.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-header": "^2.0.5",
    "gulp-imagemin": "^5.0.3",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylelint": "^8.0.0",
    "gulp-uglify": "^3.0.1",
    "gulp-wp-pot": "^2.3.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.1",
    <% if ( opts.bootstrap ) { %>
    "bootstrap": "^4.1.3"
    <% } else { %>
    "normalize.css": "^8.0.1"
    <% } %>
  }
}
