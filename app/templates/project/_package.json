{
  "name": "<%= opts.projectSlug %>",
  "title": "<%= opts.themeName %>",
  "version": "0.1.0",
  "description": "<%= opts.description %>",
  "keywords": [
    "generator-themeplate",
    "wordpress",
    "theme"
  ],
  "homepage": "<%= opts.themeURI %>",
  "license": "<%= opts.license %>",
  "private": true,
  "author": {
    "name": "<%= opts.authorName %>",
    "email": "<%= opts.authorEmail %>",
    "url": "<%= opts.authorURI %>"
  },
  "main": "style.css",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@wordpress/browserslist-config": "^2.6.0",
    "@wordpress/eslint-plugin": "^3.1.0",
    "autoprefixer": "^9.6.4",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "genetiks": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-bump": "^3.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.7",
    "gulp-imagemin": "^6.1.1",
    "gulp-load-plugins": "^2.0.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-wp-pot": "^2.3.5",
    "minimist": "^1.2.0",
    "stylelint": "^11.0.0",
    "stylelint-config-wordpress": "^15.0.0"
    <% if ( opts.bootstrap ) { %>
    "bootstrap": "^4.4.1"
    <% } else { %>
    "normalize.css": "^8.0.1"
    <% } %>
  }
}
