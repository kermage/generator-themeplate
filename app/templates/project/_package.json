{
  "name": "<%= opts.projectSlug %>",
  "title": "<%= opts.themeName %>",
  "version": "0.1.0",
  "description": "<%= opts.description %>",
  "keywords": [
    "generator-themeplate",
    "wordpress",
    "theme"
  ],
  "homepage": "<%= opts.themeURI %>",
  "license": "<%= opts.license %>",
  "private": true,
  "author": {
    "name": "<%= opts.authorName %>",
    "email": "<%= opts.authorEmail %>",
    "url": "<%= opts.authorURI %>"
  },
  "main": "style.css",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "browser-sync": "^2.26.7",
    "eslint": "^6.0.1",
    "genetiks": "^1.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-bump": "^3.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-header": "^2.0.7",
    "gulp-imagemin": "^6.0.0",
    "gulp-load-plugins": "^1.6.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-wp-pot": "^2.3.5",
    "minimist": "^1.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0",
    <% if ( opts.bootstrap ) { %>
    "bootstrap": "^4.3.1"
    <% } else { %>
    "normalize.css": "^8.0.1"
    <% } %>
  }
  "browserslist": [
    "> 1%"
  ]
}
