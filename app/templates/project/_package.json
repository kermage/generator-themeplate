    "gulp-wp-pot": "^2.5.0",
    "minimist": "^1.2.5",
    "postcss": "^8.2.4",
    "rollup": "^2.36.2",
    "sass": "^1.42.1",
    "stylelint": "^13.8.0",
    "typescript": "^4.1.3",
